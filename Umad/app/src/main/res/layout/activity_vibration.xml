<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.umad.umad.VibrationDots"
    android:background="#000000">

    <Button
        android:id="@+id/button"
        android:layout_width="100dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="53dp"
        android:layout_marginTop="33dp"
        android:layout_marginStart="19dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/button"/>

    <Button
        android:id="@+id/button19"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_below="@+id/button"
        android:layout_alignStart="@+id/button"
        android:layout_marginTop="49dp"
        android:background="@drawable/button2"/>

    <Button
        android:id="@+id/button20"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_below="@+id/button19"
        android:layout_alignStart="@+id/button19"
        android:layout_marginTop="45dp"
        android:background="@drawable/button3"/>

    <Button
        android:id="@+id/button21"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/button20"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button4"/>

    <Button
        android:id="@+id/button22"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/button"
        android:layout_alignStart="@+id/button21"
        android:background="@drawable/button5"/>

    <Button
        android:id="@+id/button23"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="15dp"
        android:layout_alignTop="@+id/button21"
        android:layout_alignParentEnd="true"
        android:background="@drawable/button6"/>

    <Button
        android:id="@+id/button24"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/button19"
        android:layout_alignStart="@+id/button23"
        android:background="@drawable/button7"/>

    <Button
        android:id="@+id/button25"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/button19"
        android:layout_alignStart="@+id/button22"
        android:background="@drawable/button8"/>

    <Button
        android:id="@+id/button26"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/button22"
        android:layout_alignStart="@+id/button24"
        android:background="@drawable/button9"/>

</RelativeLayout>
